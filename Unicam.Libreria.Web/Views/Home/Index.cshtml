@using System.Security.Claims
@{
    ViewData["Title"] = "Home Page";
}

<p>Prima View Modificata</p>
@{
    var user = User.Identity as ClaimsIdentity;
}

<table>
    <thead>
        <tr>
            <th>Nome</th>
            <th>Valore</th>
        </tr>
    </thead>
    <tbody>
@foreach(var claim in user.Claims)
{
    <tr>
        <td>@claim.Type</td>
        <td>@claim.Value</td>
    </tr>
}
    </tbody>
</table>